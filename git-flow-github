#curl -u luciano-jr https://api.github.com/user/repos -d '{"name":"luciano-jr/teste"}'

init() {
	require_git_repo
	require_gitflow_initialized
	gitflow_load_settings
	VERSION_PREFIX=$(eval "echo `git config --get gitflow.prefix.versiontag`")
	PREFIX=$(git config --get gitflow.prefix.github)
}

usage() {
	echo "usage: git flow github"
	echo "       git flow github createrepo <username-github> <repository>"
}

cmd_default() {
	echo "OH YEAH"
}

cmd_help() {
	usage
	exit 0
}

parse_args() {
	# parse options
	FLAGS "$@" || exit $?
	eval set -- "${FLAGS_ARGV}"
}

cmd_createrepo() {
	echo "$@"
}